customization:
    extraKernelArgs:
        - -init_on_alloc
        - -init_on_free
        - -selinux
        - apparmor=0
        - init_on_alloc=0
        - init_on_free=0
        - intel_iommu=on
        - iommu=pt
        - mitigations=off
        - module_blacklist=igc
        - security=none
        - talos.auditd.disabled=1
    systemExtensions:
        officialExtensions:
            - siderolabs/nfsrahead
            - siderolabs/qemu-guest-agent
